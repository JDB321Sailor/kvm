# Database Configuration
DATABASE_URL="postgresql://jetkvm:your_password_here@jetkvm-cloud-db:5432/jetkvm?schema=public"

# Google OAuth (optional for local testing)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Hostnames for OAuth callbacks
API_HOSTNAME=https://your_ip_address/api
APP_HOSTNAME=https://your_ip_address

# Cloudflare TURN (optional for local network)
# CLOUDFLARE_TURN_ID=
# CLOUDFLARE_TURN_TOKEN=

# Session Security (REQUIRED - generate with: openssl rand -base64 32)
COOKIE_SECRET="generate_with_openssl_rand_base64_32"

# File Storage (optional for basic setup)
# R2_ENDPOINT=
# R2_ACCESS_KEY_ID=
# R2_SECRET_ACCESS_KEY=
# R2_BUCKET=
# R2_CDN_URL=

# CORS Origins
CORS_ORIGINS=https://your_ip_address,http://localhost:5173

# Reverse Proxy (use if setting up nginx)
REAL_IP_HEADER=X-Real-IP

# WebRTC ICE Servers
# ICE_SERVERS=stun:stun.l.google.com:19302,stun:stun1.l.google.com:19302
